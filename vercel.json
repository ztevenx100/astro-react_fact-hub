{
  "version": 2,
  "name": "fact-hub",
  "framework": "astro",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "api/fact/[category].js": {
      "runtime": "nodejs18.x"
    },
    "api/facts/all.js": {
      "runtime": "nodejs18.x"
    },
    "api/language.js": {
      "runtime": "nodejs18.x"
    },
    "api/language/[language].js": {
      "runtime": "nodejs18.x"
    },
    "api/languages/all.js": {
      "runtime": "nodejs18.x"
    },
    "api/health.js": {
      "runtime": "nodejs18.x"
    }
  }
}
